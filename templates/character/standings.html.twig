{% extends 'base.html.twig' %}

{% block title %}Eve Toolkit Test{% endblock %}

{% block body %}
    <table>
        <thead>
            <tr>
                <td>from id</td>
                <td>from type</td>
                <td>Standing</td>
            </tr>
        </thead>
        <tbody>
        {% for elem in standings %}
            <tr>
                <td>{{ elem.from_id }}</td>
                <td>{{ elem.from_type }}</td>
                <td>{{ elem.standing }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
